<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="pane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sitandwalk.FXMLDocumentController">
    <children>
      <VBox alignment="TOP_CENTER" layoutX="-2.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
              <Button fx:id="toggleSitWalkButton" onAction="#toggleWalkSitButton" />
              <Label fx:id="label" minHeight="20.0" minWidth="69">
               <font>
                  <Font size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin></Label>
              <Label fx:id="noteLabel" minHeight="16" minWidth="69">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></Label>
            <HBox alignment="TOP_CENTER">
               <children>
                  <TextField fx:id="intervalMinutesInput" onAction="#handleUpdateTimeButton" />
                  <Button fx:id="updateIntervalButton" mnemonicParsing="false" onAction="#handleUpdateTimeButton" />
               </children>
            </HBox>
         </children>
      </VBox>
    </children>
</AnchorPane>
